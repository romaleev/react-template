{
	"name": "react-template",
	"version": "1.0.0",
	"description": "React Template",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"start": "vite",
		"build": "tsc -b tsconfig.build.json && vite build",
		"preview": "vite preview",
		"test": "npm-run-all --parallel test:component test:e2e",
		"test:component": "vitest run",
		"test:e2e": "npx playwright test --project=local",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage --reporter=text",
		"lint": "eslint {src,e2e}/**/*.ts && prettier {src,e2e}/**/*.ts",
		"lint:fix": "eslint --fix {src,e2e}/**/*.ts && prettier --write {src,e2e}/**/*.ts",
		"update": "npm i npm-check-updates@latest --save-dev && ncu --root -u && npm i && npm audit fix && npm run lint && npm test",
		"prepare": "husky && playwright install"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/romaleev/react-template.git"
	},
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"lint-staged": {
		"{client,server,types,e2e}/**/*.ts": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"keywords": [
		"react-template",
		"react",
		"typescript",
		"vite",
		"playwright",
		"i18next"
	],
	"author": "Roman Malieiev <aromaleev@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/romaleev/react-template/issues"
	},
	"homepage": "https://github.com/romaleev/react-template#readme",
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@hookform/resolvers": "^5.0.1",
		"@mui/icons-material": "^7.0.1",
		"@mui/material": "^7.0.1",
		"@tanstack/react-query": "^5.71.5",
		"dotenv": "^16.4.7",
		"i18next": "^24.2.3",
		"ky": "^1.8.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-i18next": "^15.4.1",
		"react-router-dom": "^7.4.1",
		"zod": "^3.24.2",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"@playwright/test": "^1.51.1",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.1",
		"@types/react-router-dom": "^5.3.3",
		"@vitejs/plugin-react-swc": "^3.8.1",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.5.0",
		"npm-check-updates": "^17.1.16",
		"npm-run-all": "^4.1.5",
		"playwright": "^1.51.1",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.3.0",
		"prettier-plugin-organize-imports": "^4.1.0",
		"typescript": "~5.8.2",
		"typescript-eslint": "^8.29.0",
		"vite": "^6.2.5",
		"vitest": "^3.1.1"
	}
}
